{
  "include": ["src"],
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es2017",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
